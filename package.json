{
  "name": "@cygnus/frontend-android",
  "private": true,
  "version": "0.0.0",
  "description": "Aplicaci√≥n Android de Cygnus empaquetada con Capacitor.",
  "scripts": {
    "build:web": "npm run build:native --prefix ../web",
    "cap:add": "node ./node_modules/@capacitor/cli/bin/capacitor add android",
    "sync": "node ./node_modules/@capacitor/cli/bin/capacitor sync android",
    "build": "npm run build:web && npm run sync",
    "open": "node ./node_modules/@capacitor/cli/bin/capacitor open android",
    "doctor": "node ./node_modules/@capacitor/cli/bin/capacitor doctor android"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/core": "^6.2.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "typescript": "^5.6.3"
  }
}
